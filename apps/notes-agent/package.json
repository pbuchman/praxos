{
  "name": "@intexuraos/notes-agent",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "node ../../scripts/build-service.mjs notes-agent",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
